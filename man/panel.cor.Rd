\name{panel.cor}
\alias{panel.cor}
\title{Correlation coefficient panel for pairs function}
\description{
  Correlation coefficient panel for pairs function.
}
\usage{
panel.cor(x, y, digits = 2, prefix = "", cex.cor)
}
\arguments{
  \item{x}{first data series}
  \item{y}{second data series}
  \item{digits}{number of digits to plot}
  \item{prefix}{text to prefix the coefficients}
  \item{cex.cor}{character expansion factor}
}
\author{
  Colin A. Smith, \email{csmith@scripps.edu}, based on
  pairs example code
}
\seealso{
  \code{\link{pairs}}
}
\keyword{aplot}
\keyword{internal}
