\name{na.flatfill}
\alias{na.flatfill}
\title{Fill in NA values at the extremes of a vector}
\description{
  Extend the first and last real values in a vector to fill in any
  NA values present.
}
\usage{
na.flatfill(x)
}
\arguments{
  \item{x}{numeric vector with NA values}
}
\value{
  Modified vector.
}
\author{Colin A. Smith, \email{csmith@scripps.edu}}
\keyword{NA}% at least one, from doc/KEYWORDS
\keyword{internal}
